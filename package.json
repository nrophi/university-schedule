    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.69.2",
